$('form').on('submit', function(event) {
    event.preventDefault();
    $.ajax({
        'type':   'get',
        'url' :   '/wp-content/themes/fafu-wp/ajax-search/simple-search.php',
        'data':   $(this).serialize(),
        'success': function (result) {
            $('div').html(result);
        }
    })
});